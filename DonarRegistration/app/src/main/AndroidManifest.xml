<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.donar.nfb.donarregistration" android:versionCode="1"
    android:versionName="1.0">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"

        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LoginActivity"  android:label="Welcome Donar!!" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.donar.nfb.donarregistration.activity.RegisterActivity"
            android:label="Register New Account" >
        </activity>
        <activity
            android:name="com.donar.nfb.donarregistration.activity.ContactActivity"
            android:label="Add New Contact" >
        </activity>
        <activity
            android:name="com.donar.nfb.donarregistration.activity.HomeActivity"
            android:label="Home" >
        </activity>
        <activity
            android:name="com.donar.nfb.donarregistration.activity.ContactListActivity"
            android:label="Contact List" >
        </activity>
    </application>

</manifest>